# LuxScribe Studio Environment Configuration
#
# INSTRUCTIONS:
# 1. Copy this file to .env in the project root
# 2. Replace placeholder values with your actual credentials
# 3. Make sure .env is in your .gitignore (it should be by default)

# =============================================================================
# BACKEND CONFIGURATION
# =============================================================================

# Supabase Backend Configuration
SUPABASE_URL=your_supabase_url_here
SUPABASE_SERVICE_KEY=your_supabase_service_key_here

# Database Configuration
DATABASE_URL=your_database_url_here

# Other backend variables (keep as needed)
ENVIRONMENT=development
DEBUG=true
HOST=0.0.0.0
PORT=8001
FRONTEND_HOST=localhost
FRONTEND_PORT=8082
FRONTEND_PROTOCOL=http
ALLOWED_ORIGINS=http://localhost:8080,http://localhost:8081,http://localhost:8082,http://localhost:3000
REDIS_URL=redis://localhost:6379
MINIO_ENDPOINT=localhost:9000
MINIO_ACCESS_KEY=your_minio_access_key_here
MINIO_SECRET_KEY=your_minio_secret_key_here
MINIO_BUCKET_NAME=legal-research
MINIO_SECURE=false
COURT_LISTENER_API_KEY=your_court_listener_api_key_here
CAP_API_KEY=your_cap_api_key_here
SERP_API_KEY=your_serp_api_key_here
OPENAI_API_KEY=your_openai_api_key_here
RATE_LIMIT_PER_MINUTE=60
MAX_SEARCH_RESULTS=50
MAX_CITATION_EXPANSION=15
PDF_CACHE_DAYS=30
OTEL_ENDPOINT=

# =============================================================================
# FRONTEND CONFIGURATION
# =============================================================================

# Supabase Frontend Configuration (VITE-style)
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Backend API Configuration
VITE_BACKEND_HOST=localhost
VITE_BACKEND_PORT=8001
VITE_FRONTEND_PORT=8082 